<?xml version="1.0" ?>
<component type="desktop">
  <id>kuser.desktop</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>GPL-2.0+</project_license>
  <name>KUser</name>
  <summary>User Manager</summary>
  <description>
    <p>KUser is a tool for managing users and groups on your system.</p>
    <p>Features:</p>
    <ul>
      <li>User and group creation and edition</li>
      <li>Easy password and group management for each user</li>
    </ul>
  </description>
  <url type="bugtracker">https://bugs.kde.org/enter_bug.cgi?format=guided&amp;product=kuser</url>
  <url type="help">http://docs.kde.org/stable/en/kdeadmin/kuser/index.html</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kuser.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kuser</binary>
  </provides>
</component>
