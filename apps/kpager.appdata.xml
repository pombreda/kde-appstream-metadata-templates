<?xml version="1.0" ?>
<component type="desktop">
  <id>kpager.desktop</id>
  <metadata_license>CC0-1.0</metadata_license>
  <name>KPager</name>
  <summary>Desktop Pager</summary>
  <description>
    <p>KPager gives you a thumbnail sketch of all your desktops. It is a handy tool to let you see, resize or close windows on any desktop and move windows around within or between desktops.</p>
  </description>
  <url type="homepage">http://developer.kde.org/~larrosa/kpager.html</url>
  <url type="bugtracker">https://bugs.kde.org/enter_bug.cgi?format=guided&amp;product=kpager</url>
  <url type="help">http://docs.kde.org/development/en/extragear-utils/kpager/index.html</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kpager.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kpager</binary>
  </provides>
</component>
