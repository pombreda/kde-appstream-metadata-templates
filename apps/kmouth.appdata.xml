<?xml version="1.0" ?>
<component type="desktop">
  <id>kmouth.desktop</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>GPL-2.0+</project_license>
  <name>KMouth</name>
  <summary>Speech Synthesizer Frontend</summary>
  <description>
    <p>KMouth is a program which enables persons that cannot speak to let their computer speak, e.g. mutal people or people who have lost their voice. It has a text input field and speaks the sentences that you enter. It also has support for user defined phrasebooks.</p>
  </description>
  <url type="homepage">http://www.schmi-dt.de/kmouth/index.en.html</url>
  <url type="bugtracker">https://bugs.kde.org/enter_bug.cgi?format=guided&amp;product=kmouth</url>
  <url type="help">http://docs.kde.org/stable/en/kdeaccessibility/kmouth/index.html</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kmouth.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kmouth</binary>
  </provides>
</component>
