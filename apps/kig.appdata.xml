<?xml version="1.0" ?>
<component type="desktop">
  <id>kig.desktop</id>
  <metadata_license>CC0-1.0</metadata_license>
  <name>Kig</name>
  <summary>Interactive Geometry</summary>
  <description>
    <p>Kig is an application for Interactive Geometry</p>
    <p>Features:</p>
    <ul>
      <li>Allows the students to interactively explore mathematical figures and concepts using the computer</li>
      <li>Serves as a WYSIWYG tool for drawing mathematical figures and including them in other documents</li>
    </ul>
  </description>
  <url type="bugtracker">https://bugs.kde.org/enter_bug.cgi?format=guided&amp;product=kig</url>
  <url type="help">http://docs.kde.org/stable/en/kdeedu/kig/index.html</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kig.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kig</binary>
  </provides>
</component>
