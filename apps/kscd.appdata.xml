<?xml version="1.0" ?>
<component type="desktop">
  <id>kscd.desktop</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>GPL-2.0+</project_license>
  <name>KsCD</name>
  <summary>CD Player</summary>
  <description>
    <p>KsCD is a small, fast, CDDB enabled audio CD player which supports multiple platforms.</p>
    <p>Features:</p>
    <ul>
      <li>SVG Skins support</li>
      <li>Random mode</li>
      <li>Track list</li>
    </ul>
  </description>
  <url type="bugtracker">https://bugs.kde.org/enter_bug.cgi?format=guided&amp;product=kscd</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kscd.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kscd</binary>
  </provides>
</component>
